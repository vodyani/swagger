{
  "name": "@vodyani/swagger",
  "license": "MIT",
  "version": "8.1.0-beta.1",
  "author": "ChoGathK",
  "description": "swagger",
  "homepage": "https://github.com/vodyani/swagger#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vodyani/swagger.git"
  },
  "keywords": [
    "vodyani",
    "nodejs",
    "swagger",
    "open-api",
    "javascript",
    "typescript",
    "npm-package"
  ],
  "bugs": {
    "url": "https://github.com/vodyani/swagger/issues"
  },
  "files": [
    "package.json",
    "README.MD",
    "LICENSE",
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "local": "npm i && npm i @vodyani/eslint-config -D && npx husky install",
    "build": "rm -rf dist && tsc",
    "clear": "rm -rf ./logs && rm -rf ./test/temp",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint ./src && tsc",
    "lint:fix": "eslint --fix --ext .ts src/ test/",
    "lint:html-report": "eslint ./src -f html -o eslint-report.html",
    "lint:json-report": "eslint ./src -f json -o eslint-report.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "@vodyani"
  },
  "commitlint": {
    "extends": "@commitlint/config-conventional"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/test/.spec.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "./node_modules/(?!(lodash-es|other-es-lib))"
    ]
  },
  "dependencies": {
    "@nestjs/swagger": "5.2.1",
    "@vodyani/core": "^8.6.1",
    "swagger-ui-express": "4.3.0"
  },
  "peerDependencies": {
    "@nestjs/swagger": "5.2.1",
    "swagger-ui-express": "4.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "16.3.0",
    "@commitlint/config-conventional": "16.2.4",
    "@nestjs/testing": "8.4.7",
    "@types/jest": "27.5.2",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.56",
    "@types/supertest": "2.0.12",
    "@vodyani/eslint-config": "^1.1.0",
    "husky": "7.0.4",
    "jest": "27.5.1",
    "supertest": "6.2.4",
    "ts-jest": "27.1.5",
    "typescript": "4.8.2"
  }
}
